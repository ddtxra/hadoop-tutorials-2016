LINES=`wc -l $0|cut -d ' ' -f 1`
SKIP=`grep -n '###START HERE###' $0|tail -1|cut -d ':' -f 1`
LINES=$(($LINES-$SKIP))
echo -e "\n\033[0;32m#5 SAMPLING IMPORTED DATA:\033[0m \n>>>\033[1;33m"
tail -$LINES $0
echo -e "\033[0m<<<\n\n"

###START HERE###
hdfs dfs -ls /user/`whoami`/datasets/ratings/year=1970/month=01
hdfs dfs -ls /user/`whoami`/datasets/ratings/year=1970/month=01/day=10
kite-dataset show dataset:hdfs:/user/`whoami`/datasets/ratings
